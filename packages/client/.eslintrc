{
  "extends": ["../../.eslintrc", "plugin:react/recommended", "plugin:react-hooks/recommended"],
  "plugins": ["react", "react-hooks"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/no-unknown-property": ["error",
     {"ignore": [
      "attach",
      "position",
      "side",
      "args",
      "visible",
      "userData",
      "rotation",
      "transparent",
      "castShadow",
      "metalness",
      "roughness"
     ]}]
  }
}
